#ifndef CPP_STALIN_INCLUDE_CSTDLIB__
#define CPP_STALIN_INCLUDE_CSTDLIB__

#include <stdlib.h>
#include "__config"

namespace std {
STALIN_USING_IF_EXISTS(::atof);
STALIN_USING_IF_EXISTS(::atoi);
STALIN_USING_IF_EXISTS(::atol);
STALIN_USING_IF_EXISTS(::atoll);
STALIN_USING_IF_EXISTS(::strtod);
STALIN_USING_IF_EXISTS(::strtof);
STALIN_USING_IF_EXISTS(::strtol);
STALIN_USING_IF_EXISTS(::strtold);
STALIN_USING_IF_EXISTS(::strtoll);
STALIN_USING_IF_EXISTS(::strtoul);
STALIN_USING_IF_EXISTS(::strtoull);

STALIN_USING_IF_EXISTS(::rand);
STALIN_USING_IF_EXISTS(::srand);

STALIN_USING_IF_EXISTS(::calloc);
STALIN_USING_IF_EXISTS(::free);
STALIN_USING_IF_EXISTS(::malloc);
STALIN_USING_IF_EXISTS(::aligned_alloc);
STALIN_USING_IF_EXISTS(::realloc);

STALIN_USING_IF_EXISTS(::abort);
STALIN_USING_IF_EXISTS(::atexit);
STALIN_USING_IF_EXISTS(::at_quick_exit);
STALIN_USING_IF_EXISTS(::exit);
STALIN_USING_IF_EXISTS(::getenv);
STALIN_USING_IF_EXISTS(::quick_exit);
STALIN_USING_IF_EXISTS(::system);
STALIN_USING_IF_EXISTS(::_Exit);

STALIN_USING_IF_EXISTS(::bsearch);
STALIN_USING_IF_EXISTS(::qsort);

STALIN_USING_IF_EXISTS(::abs);
STALIN_USING_IF_EXISTS(::div);
STALIN_USING_IF_EXISTS(::labs);
STALIN_USING_IF_EXISTS(::ldiv);
STALIN_USING_IF_EXISTS(::llabs);
STALIN_USING_IF_EXISTS(::lldiv);

STALIN_USING_IF_EXISTS(::mblen);
STALIN_USING_IF_EXISTS(::mbtowc);
STALIN_USING_IF_EXISTS(::wctomb);

STALIN_USING_IF_EXISTS(::mbstowcs);
STALIN_USING_IF_EXISTS(::wcstombs);

STALIN_USING_IF_EXISTS(::div_t);
STALIN_USING_IF_EXISTS(::ldiv_t);
STALIN_USING_IF_EXISTS(::lldiv_t);
STALIN_USING_IF_EXISTS(::size_t);
}

#endif //CPP_STALIN_INCLUDE_CSTDLIB__
