#ifndef CPP_STALIN_INCLUDE_CSTRING__
#define CPP_STALIN_INCLUDE_CSTRING__

#include "__config"
#ifndef STALIN_CXX_STD_FREESTANDING

#include <string.h>

namespace std {

using ::memcpy;
using ::memmove;
using ::strcpy;
using ::strncpy;

using ::strcat;
using ::strncat;

using ::memcmp;
using ::strcmp;
using ::strcoll;
using ::strncmp;
using ::strxfrm;

using ::memchr;
using ::strchr;
using ::strcspn;
using ::strpbrk;
using ::strrchr;
using ::strspn;
using ::strstr;
using ::strtok;

using ::memset;
using ::strerror;
using ::strlen;

using ::size_t;
}

#endif // ifndef STALIN_CXX_STD_FREESTANDING

#endif //CPP_STALIN_INCLUDE_CSTRING__
