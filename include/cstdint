#ifndef CPP_STALIN_SRC_CSTDINT__
#define CPP_STALIN_SRC_CSTDINT__

#include <stdint.h>

namespace std {
using ::intmax_t;
using ::uintmax_t;
using ::int16_t;
using ::uint16_t;
using ::int32_t;
using ::uint32_t;
using ::int64_t;
using ::uint64_t;
using ::int_least8_t;
using ::uint_least8_t;
using ::int_least16_t;
using ::uint_least16_t;
using ::int_least32_t;
using ::uint_least32_t;
using ::int_least64_t;
using ::uint_least64_t;
using ::int_fast8_t;
using ::uint_fast8_t;
using ::int_fast16_t;
using ::uint_fast16_t;
using ::int_fast32_t;
using ::uint_fast32_t;
using ::int_fast64_t;
using ::uint_fast64_t;
using ::intptr_t;
using ::uintptr_t;
}

#endif //CPP_STALIN_SRC_CSTDINT__
