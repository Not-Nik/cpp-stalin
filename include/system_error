#ifndef METAXX_INCLUDE_SYSTEM_ERROR__
#define METAXX_INCLUDE_SYSTEM_ERROR__

namespace std {

template <class T>
struct is_error_code_enum;

template <class T> inline constexpr bool is_error_code_enum_v = is_error_code_enum<T>::value;
}

#endif //METAXX_INCLUDE_SYSTEM_ERROR__
